{
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[javascript]": {
        "editor.codeActionsOnSave": {
            "source.fixAll": "always",
            "source.fixAll.eslint": "always",
            "source.organizeImports": "always"
        }
    },
    "[typescript]": {
        "editor.codeActionsOnSave": {
            "source.fixAll": "always",
            "source.fixAll.eslint": "always",
            "source.organizeImports": "always"
        }
    },
    "cSpell.words": [
        "alpinejs",
        "Customizer",
        "iconify",
        "simplebar",
        "tailwindcss"
    ]
}
